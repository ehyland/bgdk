    "@types/mini-css-extract-plugin": "^2.0.1",
    "@types/sade": "^1.7.3",
    "@types/serve-handler": "^6.1.1",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.1.0"
  },
  "dependencies": {
