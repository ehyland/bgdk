{
  "name": "bgdk",
  "version": "1.3.0",
  "description": "Create React App for vanilla extract apps",
  "files": [
    "bin.js",
    "/cli",
    "/dist",
    "/babel-preset",
    "/examples",
    "/examples/basic/.gitignore",
    "!/examples/*/yarn.yaml",
    "/templates/",
    "/commands/",
    "/alias/"
