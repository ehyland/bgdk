{
  "name": "bgdk",
  "version": "0.2.3",
  "description": "Create React App for vanilla extract apps",
  "files": [
    "bin.js",
    "/cli",
    "/dist",
    "/babel-preset",
    "/examples",
    "!/examples/*/yarn.lock",
    "/templates/",
    "/commands/"
