    "type": "git",
    "url": "git@github.com:ehyland/bgdk.git",
    "directory": "packages/create-bgdk"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "fs-extra": "^10.1.0",
    "globby": "^11.1.0",
    "sade": "^1.8.1",
    "v8-compile-cache": "^2.3.0"
  }
